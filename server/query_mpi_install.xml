<query id="test build status report">
  <experiment>mtt_mpi_install</experiment>

  <parameter id="p.result">
    <value>result_message</value>
    <filter>
      <all></all>
    </filter>
  </parameter>

  <source id="src.result">
    <result>success</result>
    <result>mpi_name</result>
    <result>mpi_version</result>
    <result>mpi_unique_id</result>
    <result>hostname</result>
    <result>os_version</result>
    <result>platform_type</result>
    <result>platform_id</result>
    <result>compiler_name</result>
    <result>compiler_version</result>
    <result>configure_arguments</result>
    <result>start_timestamp</result>
    <result>stop_timestamp</result>
    <input>p.result</input>
    <result>environment</result>
    <result>stdout</result>
    <result>stderr</result>
  </source>

  <output target="raw_text">
    <input>src.result</input>
  </output>
</query>
